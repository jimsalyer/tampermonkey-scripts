{
  "env": {
    "browser": true,
    "es6": true,
    "jest": true,
    "node": true
  },
  "extends": ["eslint:recommended"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "plugins": ["prettier"],
  "root": true,
  "rules": {
    "no-unused-vars": "warn",
    "prettier/prettier": "error"
  }
}
